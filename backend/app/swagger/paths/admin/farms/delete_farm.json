{
  "/api/v2/admin/farms/{id}": {
    "delete": {
      "summary": "Delete farm by ID",
      "tags": ["Admin/Farms"],
      "description": "Removes a farm from the database using its unique identifier. This is a permanent action and cannot be undone.",
      "security": [
        {
          "apiKey": []
        }
      ],
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "description": "Farm ID to be deleted",
          "required": true,
          "schema": {
            "type": "integer"
          }
        }
      ],
      "responses": {
        "200": {
          "description": "Farm deleted successfully",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "message": {
                    "type": "string",
                    "example": "Farm deleted successfully"
                  },
                  "data": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "deleted_id": {
                          "type": "string",
                          "example": "6"
                        }
                      }
                    }
                  },
                  "errors": {
                    "type": "array",
