                            "id": {
                              "type": "integer",
                              "example": 1
                            },
                            "name": {
                              "type": "string",
                              "example": "Farm 1"
                            },
                            "address": {
                              "type": "string",
                              "example": "123 Main St"
                            },
                            "latitude": {
                              "type": "string",
                              "example": "12.34"
                            },
                            "longitude": {
                              "type": "string",
                              "example": "-45.67"
                            }
                          }
                        }
                      }
                    }
                  },
                  "errors": {
