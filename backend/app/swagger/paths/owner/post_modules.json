                "type": "object",
                "properties": {
                  "message": {
                    "type": "string",
                    "example": "Module created successfully"
                  },
                  "data": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "id": {
                          "type": "integer",
                          "example": 1
                        },
                        "name": {
                          "type": "string",
                          "example": "Module 1"
                        },
                        "location": {
                          "type": "string",
                          "example": "Location 1"
                        },
                        "latitude": {
                          "type": "string",
                          "example": "12.34"
                        },
                        "longitude": {
                          "type": "string",
                          "example": "-45.67"
                        },
                        "species_fish": {
                          "type": "string",
                          "example": "Species 1"
                        },
                        "fish_quantity": {
                          "type": "string",
                          "example": "10"
                        },
                        "fish_age": {
                          "type": "string",
                          "example": "5"
                        },
                        "dimensions": {
                          "type": "string",
                          "example": "1x1x1"
                        },
                        "id_farm": {
                          "type": "integer",
                          "example": 1
                        },
                        "created_by_user_id": {
                          "type": "integer",
                          "example": 1
                        },
                        "createdAt": {
                          "type": "string",
                          "format": "date-time",
                          "example": "2025-03-04T21:55:42.000Z"
                        },
                        "updatedAt": {
                          "type": "string",
                          "format": "date-time",
                          "example": "2025-03-04T21:55:42.000Z"
                        },
                        "creator": {
                          "type": "object",
                          "properties": {
                            "id": {
                              "type": "integer",
                              "example": 1
                            },
                            "name": {
