{
  "/api/v2/owner/modules": {
    "post": {
      "tags": ["Owner/Modules"],
      "summary": "Create a new module",
      "description": "Creates a new module with the provided information.",
      "parameters": [
        {
          "in": "body",
          "name": "moduleData",
          "required": true,
          "schema": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "description": "Name of the module"
              },
              "location": {
                "type": "string",
                "description": "Location of the module"
              },
              "latitude": {
                "type": "string",
                "description": "Latitude of the module"
              },
              "longitude": {
                "type": "string",
                "description": "Longitude of the module"
              },
              "species_fish": {
                "type": "string",
                "description": "Species of fish"
              },
              "fish_quantity": {
                "type": "string",
                "description": "Quantity of fish"
              },
              "fish_age": {
                "type": "string",
                "description": "Age of the fish"
              },
              "dimensions": {
                "type": "string",
                "description": "Dimensions of the module"
              },
              "id_farm": {
                "type": "integer",
                "description": "ID of the farm"
              },
              "created_by_user_id": {
                "type": "integer",
                "description": "ID of the user who created the module"
              }
            }
          }
        }
      ],
      "security": [
        {
          "apiKey": []
        }
      ],
      "responses": {
